    "dev": "tsc -p tsconfig.build.json -w",
    "lint": "eslint ."
  },
  "dependencies": {
    "@repo/nest-prisma": "workspace:*",
    "@repo/nest-redis-cache": "workspace:*",
    "@repo/nest-shared": "workspace:*",
    "@repo/prisma": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@types/node": "^22.15.3"
  }
}

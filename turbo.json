{
  "$schema": "https://turborepo.com/schema.json",
  "globalEnv": ["ANALYZE", "NODE_ENV"],
  "globalDependencies": ["**/.env.*local", "**/.env", "**/tsconfig.json", "**/tsconfig*.json"],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^prisma:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "dependsOn": ["^prisma:generate"],
      "cache": false,
      "persistent": true,
      "env": [
        "API_URL",
        "CREDITS_SCHEDULER_BATCH_SIZE",
        "CREDITS_SCHEDULER_DRY_RUN",
        "DATABASE_URL",
        "OPENAI_API_KEY",
        "MEILISEARCH_MASTER_KEY",
        "RABBITMQ_DEFAULT_USER",
        "RABBITMQ_DEFAULT_PASS",
        "REDIS_USERNAME",
        "REDIS_PASSWORD",
        "PORT",
        "PUBSUB_EMULATOR_ENABLED",
        "PUBSUB_TOPIC_NAME_WEBHOOKS",
        "PUBSUB_TOPIC_NAME_SUB_GRANTS",
        "PUBSUB_PUSH_AUDIENCE",
        "PUBSUB_PUSH_SA_EMAIL",
        "GCP_PROJECT_ID"
      ]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true,
      "env": [
        "API_URL",
        "CREDITS_SCHEDULER_BATCH_SIZE",
        "CREDITS_SCHEDULER_DRY_RUN",
        "DATABASE_URL",
        "OPENAI_API_KEY",
        "MEILISEARCH_MASTER_KEY",
        "RABBITMQ_DEFAULT_USER",
        "RABBITMQ_DEFAULT_PASS",
        "REDIS_USERNAME",
        "REDIS_PASSWORD",
        "PORT",
        "PUBSUB_EMULATOR_ENABLED",
        "PUBSUB_TOPIC_NAME_WEBHOOKS",
        "PUBSUB_TOPIC_NAME_SUB_GRANTS",
        "PUBSUB_PUSH_AUDIENCE",
        "PUBSUB_PUSH_SA_EMAIL",
        "GCP_PROJECT_ID"
      ]
    },
    "prisma:deploy": {
      "cache": false
    },
    "prisma:generate": {
      "cache": false
    },
    "prisma:reset": {
      "cache": false
    },
    "prisma:push": {
      "cache": false
    },
    "prisma:migrate": {
      "cache": false
    },
    "prisma:studio": {
      "cache": false
    },
    "prisma:seed": {
      "cache": false
    },
    "prisma:test": {
      "cache": false,
      "dependsOn": ["prisma:generate"]
    }
  }
}
